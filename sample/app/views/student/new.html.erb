<%= form_for :student, url: student_index_path,  local: true do |form| %>
 
  <p>
    <%= form.label :name %>
    <%= form.text_field :name %>
  <p>

  <p>
    <%= form.label :email %>
    <%= form.text_field :email %>
  <p>
  
  <p>
    <%= form.label :phone %>
    <%= form.text_field :phone %>
  <p>
  
    <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', student_index_path %>
