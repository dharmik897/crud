<%= form_for :student, url: student_index_path,  local: true do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
  
  <p>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </p>
  
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', student_index_path %>
