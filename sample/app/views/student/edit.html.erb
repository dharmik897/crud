<h1>Edit article</h1>
 
<%= form_with model: @student, local: true do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
 
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>
 
  <p>
    <%= form.label :phone %><br>
    <%= form.text_field :phone %>
  </p>

  
  <p>
    <%= form.submit %>
  </p>
 
<% end %>
 
<%= link_to 'Back', student_index_path %>
